{
  "file_types": {
    "图片": [
      ".jpg",
      ".jpeg",
      ".png",
      ".gif",
      ".bmp",
      ".tiff",
      ".svg",
      ".webp",
      ".ico"
    ],
    "文档": [
      ".doc",
      ".docx",
      ".pdf",
      ".txt",
      ".rtf",
      ".odt",
      ".pages"
    ],
    "表格": [
      ".xls",
      ".xlsx",
      ".csv",
      ".ods",
      ".numbers"
    ],
    "演示文稿": [
      ".ppt",
      ".pptx",
      ".odp",
      ".key"
    ],
    "音频": [
      ".mp3",
      ".wav",
      ".flac",
      ".aac",
      ".ogg",
      ".wma",
      ".m4a"
    ],
    "视频": [
      ".mp4",
      ".avi",
      ".mkv",
      ".mov",
      ".wmv",
      ".flv",
      ".webm",
      ".m4v"
    ],
    "压缩包": [
      ".zip",
      ".rar",
      ".7z",
      ".tar",
      ".gz",
      ".bz2",
      ".xz"
    ],
    "可执行程序": [
      ".exe",
      ".msi",
      ".dmg",
      ".pkg",
      ".deb",
      ".rpm",
      ".app"
    ],
    "代码": [
      ".py",
      ".js",
      ".html",
      ".css",
      ".java",
      ".cpp",
      ".c",
      ".php",
      ".rb",
      ".go",
      ".rs"
    ],
    "字体": [
      ".ttf",
      ".otf",
      ".woff",
      ".woff2",
      ".eot"
    ]
  },
  "default_category": "其他文件",
  "organize_by_date": false,
  "date_format": "%Y-%m",
  "create_subcategories": false,
  "max_file_size": 0,
  "min_file_size": 0,
  "excluded_extensions": [
    ".tmp",
    ".temp",
    ".log",
    ".cache"
  ],
  "excluded_patterns": [
    "Thumbs.db",
    ".DS_Store",
    "desktop.ini"
  ],
  "process_hidden_files": false,
  "duplicate_strategy": "rename",
  "monitor_settings": {
    "enabled": true,
    "delay": 1,
    "recursive": false
  },
  "logging": {
    "level": "INFO",
    "file": "logs/file_organizer.log",
    "max_size": 10485760,
    "backup_count": 5
  },
  "ui_settings": {
    "theme": "default",
    "language": "zh_CN",
    "window_size": "800x600",
    "remember_paths": true
  },
  "recent_paths": {
    "source_dirs": [],
    "target_dirs": []
  },
  "advanced": {
    "use_file_hash": true,
    "hash_algorithm": "md5",
    "chunk_size": 4096,
    "max_hash_file_size": 1048576,
    "preserve_timestamps": true,
    "create_shortcuts": false
  }
}